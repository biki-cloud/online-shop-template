{
  "name": "online-shop-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "local:setup": "npx tsx lib/local-setup/setup.ts",
    "db:seed": "npx tsx lib/infrastructure/db/seed.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx lib/infrastructure/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:reset": "drizzle-kit generate && drizzle-kit push && pnpm db:seed",
    "db:generate:prd": "dotenv -e .env.prd  drizzle-kit generate",
    "db:migrate:prd": "dotenv -e .env.prd  npx tsx lib/infrastructure/db/migrate.ts",
    "db:push:prd": "dotenv -e .env.prd  drizzle-kit push",
    "db:studio:prd": "dotenv -e .env.prd  drizzle-kit studio",
    "db:reset:prd": "dotenv -e .env.prd  drizzle-kit generate && dotenv -e .env.prd  drizzle-kit push && dotenv -e .env.prd  pnpm db:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev:test": "next dev -p 3010",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen http://localhost:3000 --output e2e/generated.spec.ts",
    "dev:clean": "rm -rf .next && pnpm dev"
  },
